<div class="main-content">
   <section class="section">
      <div class="section-header">
         <h1>Nuevo Cliente</h1>
      </div>
   </section>
   <div class="col-12 col-md-6 col-lg-6 mx-auto">
   <div class="card">
   <%= form_with model: @client do |f| %>

      <div class="card-body">
         <div class="form-row">
            <div class="form-group col-md-6">
               <%= f.label :first_name %>
               <%= f.text_field "Nombre", class: "form-control", placeholder: "Nombre"%>
               
            </div>
            <div class="form-group col-md-6">
                <%= f.label :last_name %>
               <%= f.text_field "Apellido", class: "form-control", placeholder: "Apellido"%>
            </div>
         </div>
         <div class="form-group">
             <%= f.label :address %>
               <%= f.text_field "Direccion", class: "form-control", placeholder: "Direccion"%>
         </div>
         <div class="form-row">
         <div class="form-group col-md-6">
              <%= f.label :sec_no %>
               <%= f.text_field "Identificacion", class: "form-control", placeholder: "Identificacion"%>
           
               
            </div>
            <div class="form-group col-md-6">
                <%= f.label :phone %>
               <%= f.number_field "Telefono", class: "form-control", placeholder: "Telefono"%>
            </div>
         </div>
             
         <div class="form-row">
            <div class="form-group col-md-6">
               <label for="inputCity">Ciudad</label>
               <input type="text" class="form-control" id="inputCity">
            </div>
            <div class="form-group col-md-4">
               <label for="inputState">Estado</label>
               <select id="inputState" class="form-control">
                  <option selected>Choose...</option>
                  <option>...</option>
               </select>
            </div>
            <div class="form-group col-md-2">
               <label for="inputZip">Zip</label>
               <input type="text" class="form-control" id="inputZip">
            </div>
         </div>
         
          <div class="">
         <%= f.submit "Crear Cliente", class: "btn btn-danger m-3 float-right" %>
         </div>
         </div>
        
      </div>
      <% end %>
   </div>
</div>